    "build": "parcel build index.js --no-minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Firenzia/sakura-ui.git"
  },
  "keywords": [
    "UI",
    "vue"
  ],
  "author": "Firenzia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Firenzia/sakura-ui/issues"
  },
  "homepage": "https://github.com/Firenzia/sakura-ui#readme",
  "dependencies": {
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.6.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "karma": "^4.0.1",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^6.1.3",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.18.0",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0",
    "vue-template-compiler": "^2.6.10"

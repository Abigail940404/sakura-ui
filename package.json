{
  "name": "heian-sakura-ui",
  "version": "0.0.3",
  "description": "an UI framework based on Vue.js",
  "main": "dist/index.js",
  "scripts": {
    "start": "parcel index.html --no-cache",
    "dev-test": "parcel watch test/* --no-cache & karma start",
